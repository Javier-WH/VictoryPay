<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register.Register4"
    tools:layout_editor_absoluteY="81dp">

    <ScrollView
        android:id="@+id/scrollView5"
        android:layout_width="21dp"
        android:layout_height="31dp"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
    </ScrollView>

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="Datos del contacto"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnBack4"
        android:layout_width="186dp"
        android:layout_height="59dp"
        android:layout_marginStart="10dp"
        android:text="Atras"
        app:icon="@android:drawable/ic_media_rew"
        app:layout_constraintBottom_toBottomOf="@+id/scrollView5"
        app:layout_constraintEnd_toStartOf="@+id/scrollView5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/scrollView5" />

    <Button
        android:id="@+id/btnNext4"
        android:layout_width="0dp"
        android:layout_height="59dp"
        android:layout_marginStart="10dp"
        android:layoutDirection="rtl"
        android:text="Siguiente"
        app:icon="@android:drawable/ic_media_ff"
        app:layout_constraintBottom_toBottomOf="@+id/scrollView5"
        app:layout_constraintEnd_toStartOf="@+id/scrollView5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/scrollView5" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:text="Teléfono de contacto"
        app:layout_constraintBottom_toTopOf="@+id/TextPhone1"
        app:layout_constraintStart_toStartOf="@+id/TextPhone1" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:text="Teléfono opcional"
        app:layout_constraintBottom_toTopOf="@+id/TextPhone2"
        app:layout_constraintStart_toStartOf="@+id/TextPhone2" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="355dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:text="E-mail"
        app:layout_constraintBottom_toTopOf="@+id/TextEmailAddress"
        app:layout_constraintStart_toStartOf="@+id/TextEmailAddress" />

    <EditText
        android:id="@+id/TextPhone1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:hint="Numero de teléfono"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView5" />

    <EditText
        android:id="@+id/TextPhone2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:hint="Numero de teléfono"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView5" />

    <EditText
        android:id="@+id/TextEmailAddress"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="190dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:hint="Dirrección de correo electrónico"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView5" />

    <Switch
        android:id="@+id/chkWhatsaap1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:layoutDirection="rtl"
        android:pointerIcon="context_menu"
        android:text="Whatsaap"
        android:textColor="@color/teal_700"
        app:layout_constraintBottom_toTopOf="@+id/TextPhone1"
        app:layout_constraintStart_toStartOf="@+id/TextPhone1" />

    <Switch
        android:id="@+id/chkWhatsaap2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layoutDirection="rtl"
        android:pointerIcon="context_menu"
        android:text="Whatsaap"
        android:textColor="@color/teal_700"
        app:layout_constraintBottom_toTopOf="@+id/TextPhone2"
        app:layout_constraintStart_toStartOf="@+id/TextPhone2" />

</androidx.constraintlayout.widget.ConstraintLayout>